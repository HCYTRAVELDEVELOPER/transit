.detalle_service {
    position: absolute;
    z-index: 23;
    top: 4.2rem;
    left: 0.6rem;
    width: 302px;
    height: auto;
    border-radius: 10px;
    background: #be2f2cc2;
    max-height: 508px;
    padding: 17px;
    box-sizing: border-box;
    overflow: auto;
    color: white;
    box-shadow: 0px 0px 5px 1px grey;
}
.detalle_conductor::-webkit-scrollbar {
    width: 5px;     /* Tamaño del scroll en vertical */
    height: 8px;    /* Tamaño del scroll en horizontal */
    /*display: none;   Ocultar scroll */
}
.detalle_conductor::-webkit-scrollbar-thumb {
    background: #921717;
    border-radius: 4px;
}

/* Cambiamos el fondo y agregamos una sombra cuando esté en hover */
.detalle_conductor::-webkit-scrollbar-thumb:hover {
    background: #f53636;
    box-shadow: 0 0 2px 1px rgba(0, 0, 0, 0.2);
}

/* Cambiamos el fondo cuando esté en active */
.detalle_conductor::-webkit-scrollbar-thumb:active {
    background-color: #999999;
}
.detalle_conductor::-webkit-scrollbar-track {
    background: #e1e1e1;
    border-radius: 4px;
}

/* Cambiamos el fondo cuando esté en active o hover */
.detalle_conductor::-webkit-scrollbar-track:hover,
.detalle_conductor::-webkit-scrollbar-track:active {
    background: #d4d4d4;
}
.detalle_conductor {
    position: absolute;
    z-index: 23;
    top: 0.2rem;
    left: 0.2rem;
    width: 97%;
    height: 98%;
    border-radius: 10px;
    background: #b7b4b4;
    padding: 17px;
    box-sizing: border-box;
    overflow: auto;
}
