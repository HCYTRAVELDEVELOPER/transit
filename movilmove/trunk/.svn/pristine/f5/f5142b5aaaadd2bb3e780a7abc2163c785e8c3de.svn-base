/*/////////////////////////PARADAS ADICIONALES ///////////////////////////*/

.list-paradas {
    position: fixed;
    /*background: #bd1313;*/
    background-color: #29bd29;
    list-style: none;
    border-radius: 8px;
    color: #fff;
    z-index: 111111;
    box-sizing: border-box;
    overflow: auto;
    margin: auto;
    width: 100%;
    height: auto;
    top: auto;
    left: 0px;
    bottom: 2px;
    right: auto;
    padding: 0;
}
.parada_list_show{
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    bottom: auto;
    right: auto;
    /*max-height: calc(100% - 75px);*/
    /*max-height: calc(100% - 105px);*/
    /*box-shadow: 0px 0px 15px #000;*/
    /*top: 10px;*/
    /*background-color: transparent;*/
    background-color: rgb(0 0 0 / 52%);
}
.containtParadaItms{
    position: relative;
    max-height: calc(100% - 100px);
    height: 100%;
    overflow: auto;
}
.containtShade{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 140px;
    z-index: 100000000;
    background: rgb(0,0,0);
    background: linear-gradient(0deg, rgba(0,0,0,1) 0%, rgba(0,0,0,0) 100%);
    background: linear-gradient(0deg, rgba(0,0,0,1) 10%, rgba(0,0,0,0.9) 73%, rgba(0,0,0,0) 100%);
    background: linear-gradient(0deg, rgba(0,0,0,1) 10%, rgba(0,0,0,0.9) 60%, rgba(0,0,0,0) 100%);
    text-align: center;
    box-shadow: 0px -3px 14px 1px rgb(0 0 0 / 40%);
    padding: 29px 10px 26px;
    height: auto;
    font-weight: bold;
    font-size: 15px;
    box-sizing: border-box;
    min-height: 100px;
    /*background-color: rgb(0 0 0 / 88%);*/
}
.list-paradas-close, .containtShade, .containtParadaItms{
    display: none;
}
.parada_list_open{
    display: none;
}
.parada_list_show .list-paradas-close,
.parada_list_show .containtShade,
.parada_list_show .containtParadaItms
{
    display: block;
}
.parada_list_show .list-paradas-close{
    position: fixed;
    top: 2px;
    right: 5px;
    background-color: #ffffff;
    color: #000;
    width: 35px;
    height: 35px;
    text-align: center;
    border-radius: 50%;
    line-height: 50px;
    cursor: pointer;
    border: 1px solid gray;
    box-shadow: 0px 0px 5px #000;
    z-index: 111;
}
.parada_list_show .list-paradas-count{
    display: none;
}
.list-paradas-item{
    display: none;
    background: #fcfcfc;
    padding: 11px;
    border-bottom: 1px solid #3e3c3c;
    color: #000;
    border-radius: 10px;
    margin-bottom: 10px;
    flex-wrap: wrap;
    justify-content: center;
    margin-top: 12px;
}
.parada_list_show .list-paradas-item{
    display: flex;
}
.list-paradas-close .material-icons{

}
.ver_parada {
    /*background: #11a711;*/
    background-color: #29bd29;
    width: 100%;
    border-radius: 10px;
    text-align: center;
    color: #fff;
    padding: 10px 20px;
    margin: 5px;
    font-size: 17px;
    font-weight: bold;
}
